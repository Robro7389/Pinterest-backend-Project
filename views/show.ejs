<% include ./partials/header.ejs  %>
   <div class="w-full min-h-screen bg-white px-10 py-10 text-black font-medium">
     <div class="cards flex flex-wrap gap-12">
		 <% user.posts.forEach(function(elem){ %>
		    <div class="card w-52">
				<div class="w-full h-42 bg-sky-200 rounder-lg overflow-hidden">
					<img src="/images/uploads/<%= elem.image %>" alt="">
				</div>
				<h5 class="mt-2 text-sm"><%= elem.title %></h5>
			</div>
		<% }) %>
	 </div>

   </div>