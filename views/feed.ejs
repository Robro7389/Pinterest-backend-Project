<% include ./partials/header.ejs  %>
   <div class="w-full min-h-screen bg-white px-15 py-5 font-semibold text-black">
     <div class="cards flex flex-wrap gap-12">
		 <% post.forEach(function(elem){ %>
		    <div class="card w-52 ">
				<div class="w-full h-42 bg-zinc-700 rounder-lg overflow-hidden">
					<img class="w-full h-full object-cover" src="/images/uploads/<%= elem.image %>" alt="">
				</div>
				<h5 class="mt-2 text-sm"><%= elem.title %></h5>
				<h6 class="text-sm mt-1 opacity-60">@<%= elem.user.username %></h6>
			</div>
		<% }) %>
	 </div>

   </div>